# 1999 年 9 月 21 日 1 時 47 分 16 秒
# 2018 年 2 月 6 日 23 時 50 分 42 秒
time1 <- as.POSIXct("1999-09-21 01:47:16", tz = "GMT")
time2 <- as.POSIXct("2018-02-06 23:50:42", tz = "GMT")
interval <- as.integer(time2) - as.integer(time1)
r_year <- as.integer(interval / 31104000)
r_month <- as.integer((interval %% 31104000) / 2592000)
r_day <- as.integer((interval %% 2592000) / 86400)
r_hour <- as.integer((interval %% 86400) / 3600)
r_cent <- as.integer((interval %% 3600) / 60)
r_sec <- as.integer((interval %% 60))
cat(r_year, "年", r_month, "月", r_day, "日")
cat(r_hour, "時", r_cent, "分", r_sec, "秒")
